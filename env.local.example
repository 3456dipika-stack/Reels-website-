# MongoDB connection string for the auth database
# In your local environment, rename this file to .env.local
# and replace the placeholder URIs with your actual MongoDB URIs.
MONGODB_URI_AUTH="your_mongodb_uri_for_auth"

# MongoDB connection string for the users database
MONGODB_URI_USERS="your_mongodb_uri_for_users"

# MongoDB connection string for the reels database
MONGODB_URI_REELS="your_mongodb_uri_for_reels"

# JWT Secret for signing tokens
JWT_SECRET="your_jwt_secret"